@import "reset.css";
/*@import "function.css";*/
/*

-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr=#99ffffff,endColorstr=#99ffffff)";  
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#99ffffff,endColorstr=#99ffffff);
background:rgba(255,255,255,.8);


filter:alpha(opacity=40); 针对 IE8 以及更早的版本 */

body
	font-size 100%
.taobao-1-27
	.w
		width 1000px
		margin 0 auto
	.header
		.bgf7f7f7
			padding 8px 0
			color #000
			font-size 12px
			line-height 1
			background #f7f7f7
			border-top 1px solid #cccccc
			.line
				padding 0 10px
				line-height 0.9
				color #000
			.icon
				padding-left 20px
				background #fff url() left center no-repeat
				margin-left 10px
		.brand
			width 400px
			height 100px
			background url(../img/hico3.png) left center no-repeat
		.search-box
			padding-top 30px
			font-size 0
			.text
				padding-left 10px
				width 250px
				height 30px
				font-size 12px
				color #666
				line-height 30px
				border 1px solid #3390DD
				vertical-align middle
				outline none
			.submit
				width 30px
				height 30px
				outline none
				vertical-align middle
				border none
				background #d94b35 url(../img/hico4.png) center center no-repeat
		.nav
			font-size 22px
			color #fff
			line-height 36px
			a
				width 14%
				text-align center
				&:hover
					background #fff
					color #000
			background #1369c0
	
	.main
		padding 30px 0
		background #f7f7f7
		&>.w
			overflow hidden
		.swiper-container
			width 660px
			height 360px
			.swiper-slide
				position relative
				.text
					position absolute
					bottom 0
					left 0
					right 0
					font-size 14px
					color #fff
					line-height 30px
					padding-left 20px
					background #666
					background rgba(0,0,0,0.5)
			.swiper-pagination
				right 10px
				bottom 11px
				font-size 0
				left auto
				width auto
				text-align right
				span
					width 10px
					height 10px
					opacity 1
					background #fff
					display inline-block
					margin 0 5px
					border-radius 50%
					*display inline
					*zoom 1
					&.swiper-active-switch
						background #0068b7
		.box-shadow
			box-shadow 2px 2px 2px #ccc
		.pic-news
			width 340px
			.title
				border-bottom 2px solid #cdcdcd
				.item
					position relative
					font-size 24px
					color #1563c5
					line-height 38px
					text-align center
					width 50%
					&:after
						content ""
						position absolute
						bottom -2px
						left 0 
						right 0
						border-top 2px solid #1563c5
					span
						padding-left 20px
						background url(../img/round.jpg) left center no-repeat
			.content
				background #fff
				.item
					padding-top 10px
					li
						float none
						padding-left 37px
						padding-bottom 10px
						font-size 14px
						color #4C4C4C
						line-height 20px
						background url(../img/round.jpg) 20px 8px/4px 4px no-repeat
						&:hover
							a
								// font-weight bold
								color #db493a
						// 	display block
						// 	width 100%
						// 	overflow hidden
						// 	text-overflow ellipsis
						// 	white-space nowrap

		.column
			&.w665
				width 665px
				padding-bottom 5px
			&.w315
				width 315px
			.h3-title
				padding 30px 0 10px
				font-size 22px
				color #1563C5
				font-weight bold
			.w50
				width 325px
				.tabs-wrap
					padding 8px 20px 20px
					background #fff
					.tabs-control
						border-bottom 2px solid #e3e3e3
						.item
							position relative
							font-size 18px
							color #6b6b6b
							line-height 34px
							margin-right 15px
							cursor pointer
							&:hover,
							&.active
								color #010101
								&:after
									content ""
									position absolute
									bottom -2px
									left 0
									right 0
									border-top 2px solid #116aba
					.tabs-content
						.item
							padding-top 5px
							li
								margin-top 15px
								padding-left 10px
								font-size 14px
								color #333
								background url(../img/2750989.gif) left center no-repeat
								&:hover,
								&.active
									color #db493a
									background-image url(../img/830081.gif)
								a
									display block
									width 100%
									overflow hidden
									text-overflow ellipsis
									white-space nowrap
						
		
		.w100
			margin-top 15px
			background #fff
			border 1px solid #dcdcdc
			&.list-page
				margin-top 0
				.tabs-content
					.item
						padding 0
						ul
							margin 0 20px 
							width auto
							padding 15px 0 0
							border-bottom 1px dashed #D9D9D9
						.page-control
							padding 20px 0
							font-size 0
							text-align center
							float left
							width 100%
							a
								width auto
								float none
								font-size 14px
								color #000
								padding 0 3px
								display inline-block
								*display inline
								*zoom 1
								&:hover,
								&.active
									color #FF003B
			.tabs-control
				background #f7f7f7
				border-bottom 1px solid #1169c0
				.item
					position relative
					width 120px
					text-align center
					font-size 16px
					color #06c
					line-height 30px
					border-top 2px solid #f7f7f7
					border-right 1px solid transparent
					border-left 1px solid transparent
					cursor pointer
					&:hover
					&.active
						border-right-color #cccccc
						border-left-color #cccccc
						border-top-color #06c
						background #fff
						&:first-child
							border-left-color transparent
							&:after
								left -1px
						&:after
							position absolute
							bottom -1px
							left 0
							right 0
							content ""
							border-top 1px solid #fff
			.tabs-content
				.item
					padding 15px 20px 0 20px
					overflow hidden
					ul
						float left
						width 100%
					li
						margin-bottom 14px
						padding-left 10px
						float left
						width 100%
						box-sizing border-box
						background url(../img/2750989.gif) left center no-repeat
						&:hover,
							&.active
								background-image url(../img/830081.gif)
								a
									color #db493a
					a
						display block
						color #4C4C4C
						font-size 14px
						line-height 1
						float left
						width 100%
						.w80
							width 80%
							float left
							white-space nowrap
							text-overflow ellipsis
							overflow hidden
						.tips
							float left
							background #0062d5
							font-size 12px
							color #fff
							line-height 1
							padding 2px 5px
							margin-top -2px
						.w20
							float right
							width 10%
							text-align right
		.tabs5
			.tabs-content
				.item
					li
						width 16.6%
						box-sizing border-box
		.column.w315
			.h3-title
				text-align center
			.other
				background #e7e8ed
				padding 0 2.5% 15px
				margin 0 auto
				a
					margin-top 15px
					width 33.33%
					img
						display block
						margin 0 auto
			.new
				margin-top 5px
				background #e6e8ed
				border 1px solid #cbcdd2
				&.list-page
					margin-top 0
				.title
					padding-left 20px
					font-size 18px
					color #000
					line-height 34px
					background #d5d6d9
					border-bottom 1px solid #1369c0
					span
						display inline-block
						*display inline
						*zoom 1
						position relative
						&:after
							content ""
							position absolute
							bottom 0
							left -10px
							right -10px
							border-top 3px solid #1369c0
				.content
					padding 15px 15px 0 15px
					li
						padding-bottom 5px
						a
							display block
							width 100%
							overflow hidden
							text-overflow ellipsis
							white-space nowrap
							font-size 14px
							color #616161
							line-height 2
							padding-left 10px
							box-sizing border-box
							background url(../img/2750989.gif) left center no-repeat
							&:hover,
							&.active
								color #db493a
								background-image url(../img/830081.gif)
			.guide
			.rule
			.be-on-duty
			.btn
				background #e7e8ed
			.guide
				padding 10px 20px 20px 
				.title
					font-size 16px
					color #1667b6
					line-height 32px
					padding-left 5px
					border-bottom 2px solid #1667b6
				.content
					a
						margin-top 20px
						display block
						font-size 22px
						color #fff
						line-height 40px
						width 100%
						background #4589dc
						text-align center
						border-radius 5px
			.rule
				padding 0 13px
				.title
					background #fff
					padding 0 10px
					font-size 18px
					color #000
					line-height 37px
				.content
					background #fff
					padding 0 10px 10px
					.text
						width 100%
						height 46px
						box-sizing border-box
						padding-left 5px
						font-size 12px
						color #666666
						line-height 30px
						border 8px solid #e4e4e4
						background #fff
						outline none
					.hot
						padding 10px
						margin-top 5px
						font-size 14px
						line-height 1.5
						color #000
						background #e4e4e4
						.sub-title
							width 65px
							font-size 18px
							color #045FA5
							text-align right
							background none
						.line
							padding 0 5px
			.be-on-duty
				padding 20px 20px 0 
				.title
					font-size 16px
					color #1667b6
					line-height 32px
					padding-left 5px
					border-bottom 2px solid #1667b6
				.content
					padding 10px 10px 0
					font-size 14px
					color #000
					line-height 2
			.btn
				padding 20px 0
				// padding-bottom 500px
				// margin-bottom -505px
				a
					display block
					width 250px
					height 40px
					font-size 22px
					color #fff
					font-weight bold
					line-height 40px
					text-align center
					background #4096ee
					box-shadow 0 0 0 3px #DEEBFF
					border-radius 5px
					margin 0 auto
	
	.main.child
		padding-top 0
		background #f7f7f7
		&>.w
			overflow visible
		.nav
			padding 10px 0
			font-size 12px
			a
				padding 0 10px
				color #1369c0
		.child-content
			padding 40px 90px 20px
			background #fff
			border 1px solid #dbdbdb
			.h1
				font-size 30px
				color #323431
				line-height 1.5
				text-align center
			.more-info
				padding 20px 0
				text-align center
				span
					font-size 12px
					color #989898
					padding 0 20px
			.set-font-size
				font-size 12px
				color #000
				text-align center
				a
					padding 0 5px
			.content-body
				padding 20px
				font-size 20px
				margin 20px 0
				border-top 1px solid #F2F2F2
				border-bottom 1px solid #F2F2F2
				&.big
					font-size 26px
				&.small
					font-size 16px
				.h2
					font-size 100%
					color #000
					font-weight bold
					line-height 2
					span
						font-weight normal
				p
					font-size 90%
					color #000
					line-height 2
					a
						color #00F
				.row
					padding-top 60px
				.error
					font-size 18px
					color #1369C0
					line-height 1
				.share
					padding 4px 2px 0 0
					width 264px
					height 32px
					box-sizing border-box
					background url(../img/hico11.png) center center no-repeat
					a
						width 24px
						height 24px
						margin-left 10px
			.content-footer
				padding 0 20px
				.title
					font-size 18px
					color #2C79C7
					padding-bottom 10px
				.list
					padding-left 10px
					a
						display block
						margin-bottom 10px
						width 100%
						padding-left 10px
						white-space nowrap
						text-overflow ellipsis
						overflow hidden
						background url(../img/2750989.gif) left center no-repeat
						&:hover,
						&.active
							color #db493a
							background-image url(../img/830081.gif)
	.footer
		padding 20px 0 40px
		text-align center
		font-size 18px
		color #fff
		line-height 2
		background #1369c0
